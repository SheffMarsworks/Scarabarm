        <group name="gripper" />
    </group>
-->


    <end_effector name="tool" parent_link="tool0" group="arm"/>

    <group_state name="home" group="arm">
        <joint name="joint_1_to_joint_2" value="0"/>
        <joint name="joint_2_to_link_2" value="0"/>
        <joint name="link_2_to_joint_3" value="0"/>
        <joint name="joint_4_to_joint_5" value="0"/>
        <joint name="joint_5_to_joint_6" value="0"/>
        <joint name="joint_6_to_tool0" value="0"/>
    </group_state>

    <group_state name="ready" group="arm">
        <joint name="joint_1_to_joint_2" value="0"/>
        <joint name="joint_2_to_link_2" value="0"/>
        <joint name="link_2_to_joint_3" value="-1.57"/>
        <joint name="joint_4_to_joint_5" value="0"/>
        <joint name="joint_5_to_joint_6" value="1.57"/>
        <joint name="joint_6_to_tool0" value="0"/>
    </group_state>

