    <!--
      Pairs of links in the robot model for which collision checking should be disabled
      during motion planning and simulation.

      Adjacent: Collision checking is unnecessary because the links are always in contact.

      Never: Collision checking is unnecessary because the robotic arm is designed in a way that
        they will never collide with each other.
    -->

    <disable_collisions link1="base_link" link2="joint_1_1" reason="Adjacent"/>
    <disable_collisions link1="joint_1_1" link2="joint_2_1" reason="Adjacent"/>
    <disable_collisions link1="joint_2_1" link2="link_2_1" reason="Adjacent"/>
    <disable_collisions link1="link_2_1" link2="joint_3_1" reason="Adjacent"/>
    <disable_collisions link1="joint_3_1" link2="link_3_1" reason="Adjacent"/>
    <disable_collisions link1="link_3_1" link2="joint_4_1" reason="Adjacent"/>
    <disable_collisions link1="joint_4_1" link2="joint_5_1" reason="Adjacent"/>
    <disable_collisions link1="joint_5_1" link2="joint_6_1" reason="Adjacent"/>
    <disable_collisions link1="joint_6_1" link2="tool0" reason="Adjacent"/>

    <disable_collisions link1="base_link" link2="joint_2_1" reason="Never"/>
    <disable_collisions link1="base_link" link2="link_2_1" reason="Never"/>

